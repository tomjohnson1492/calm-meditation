---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:media="http://search.yahoo.com/mrss/" version="2.0">
   <channel>
      <title>{{ site.title }}</title>
      <link>{{ site.url | append: site.baseurl }}</link>
      <description>{{ site.description }}</description>
      {% for item in site.posts %}
      <item>
         <title>{{ item.title }}</title>
         <link>{{ item.url | prepend: site.baseurl | prepend: site.url }} </link>
         <description>{{ ite.description }}</description>
         <guid isPermaLink="false">{{ item.url | prepend: site.baseurl | prepend: site.url }} </guid>
         <media:content url="{{ item.media_url | prepend: site.baseurl_media }}" fileSize="{{ item.filesize }}" type="video/mpeg" height="1280" width="720" duration="{{ item.duration }}" medium="video" isDefault="true">
            <media:title>{{ item.title }}</media:title>
            <media:description>{{ item.description }}</media:description>
            <media:thumbnail url="{{ item.media_thumbnail | prepend: "/images/thumbs/" | prepend: site.url }}" />
         </media:content>
      </item>
      {% endfor %}
   </channel>
</rss>
